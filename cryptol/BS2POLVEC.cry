BS2POLVEC : [SABER_POLYVECCOMPRESSEDBYTES][8] -> [SABER_K][SABER_N][16]
BS2POLVEC bytes = [ BS2POL (take chunk bytes) | chunk <- chunks ]
  where
    chunks = [0, SABER_POLYCOMPRESSEDBYTES .. SABER_POLYVECCOMPRESSEDBYTES - SABER_POLYCOMPRESSEDBYTES]

BS2POL : [SABER_POLYCOMPRESSEDBYTES][8] -> [SABER_N][16]
